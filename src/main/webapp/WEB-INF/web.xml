<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CartServlet</servlet-name>
  	<display-name>CartServlet</display-name>
  	<description></description>
  	<servlet-class>com.user.servlet.CartServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RemoveBookCart</servlet-name>
  	<display-name>RemoveBookCart</display-name>
  	<description></description>
  	<servlet-class>com.user.servlet.RemoveBookCart</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CartServlet</servlet-name>
  	<url-pattern>/CartServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RemoveBookCart</servlet-name>
  	<url-pattern>/RemoveBookCart</url-pattern>
  </servlet-mapping>
</web-app>
